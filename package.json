{
  "name": "@gabortorma/nuxt-eslint-layer",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b",
  "description": "Preconfigured eslint for Nuxt 3",
  "author": "GaborTorma",
  "license": "MIT",
  "homepage": "https://github.com/GaborTorma/nuxt-eslint-layer",
  "repository": {
    "type": "github",
    "url": "git+ssh://git@github.com/GaborTorma/nuxt-eslint-layer.git"
  },
  "bugs": "https://github.com/GaborTorma/nuxt-eslint-layer/issues",
