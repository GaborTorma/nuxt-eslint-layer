{
  "name": "@gabortorma/nuxt-eslint-layer",
  "type": "module",
  "version": "0.5.3",
  "packageManager": "pnpm@10.13.1",
  "description": "Preconfigured eslint for Nuxt 3",
  "author": "GaborTorma",
  "license": "MIT",
  "homepage": "https://github.com/GaborTorma/nuxt-eslint-layer",
  "repository": {
    "type": "github",
    "url": "git+ssh://git@github.com/GaborTorma/nuxt-eslint-layer.git"
  },
  "bugs": "https://github.com/GaborTorma/nuxt-eslint-layer/issues",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nuxt",
    "layer",
    "eslint"
  ],
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi dev .playground",
    "build": "nuxi build .playground",
    "prepare": "nuxt prepare .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "vue-tsc --noEmit",
    "release": "dotenv release-it"
  },
  "peerDependencies": {
    "nuxt": "^4.0.0",
    "vue": "^3.5.0"
  },
  "dependencies": {
    "@gabortorma/antfu-eslint-config": "catalog:",
    "@nuxt/eslint": "catalog:",
    "@typescript-eslint/utils": "catalog:",
    "@vue/compiler-sfc": "catalog:",
    "vite-plugin-eslint2": "catalog:",
    "vue-tsc": "catalog:"
  },
  "devDependencies": {
    "@gabortorma/mwm": "catalog:",
    "@types/node": "catalog:",
    "dotenv-cli": "catalog:",
    "nuxi": "catalog:",
    "nuxt": "catalog:",
    "release-it": "catalog:",
    "release-it-pnpm": "catalog:",
    "typescript": "catalog:",
    "vue": "catalog:"
  }
}
